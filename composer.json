{
    "name": "zenstruck/mailer-test",
    "description": "Alternative, opinionated helpers for testing emails sent with symfony/mailer.",
    "homepage": "https://github.com/zenstruck/mailer-test",
    "type": "library",
    "license": "MIT",
    "keywords": ["mailer", "symfony", "test"],
    "authors": [
        {
            "name": "Kevin Bond",
            "email": "kevinbond@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/framework-bundle": "^4.4|^5.0",
        "symfony/mailer": "^4.4|^5.0",
        "symfony/polyfill-php80": "^1.23.1",
        "zenstruck/assert": "^1.0",
        "zenstruck/callback": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.0",
        "symfony/messenger": "^4.4|^5.0",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/var-dumper": "^4.4|^5.0",
        "symfony/yaml": "^4.4|^5.0",
        "zenstruck/browser": "^1.0@dev"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "Zenstruck\\Mailer\\Test\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Zenstruck\\Mailer\\Test\\Tests\\": "tests/" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
